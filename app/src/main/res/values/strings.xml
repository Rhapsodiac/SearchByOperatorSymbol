    <!--HELP STRINGS FOR OPERATORS-->

    <string-array
        name="oper_text">
        <item>\u002B</item>
        <item>\@</item>
        <item>$</item>
        <item>#</item>
        <item>-</item>
        <item>\"\"</item>
        <item>*</item>
        <item>..</item>
        <item>site:</item>
        <item>link:</item>
        <item>related:</item>
        <item>OR</item>
        <item>info:</item>
        <item>cache:</item>
    </string-array>

    <string-array
        name="oper_help_array">
        <item>Searches for Google\+ pages or blood types. Example: \+Chrome or AB\+.</item>
        <item>Find social tags. Examples: \@somegoogler.</item>
        <item>Find prices. Example: fitbit $400</item>
        <item>Find popular hashtags for trending topics. Example: #GamerGate</item>
        <item>Excludes sites with the attached word. Example: jaguar -cat</item>
        <item>Includes only sites with the contained words. Example: star wars \"no spoilers\"</item>
        <item>Acts as a placeholder for wildcard terms. Example: a * in the hand is worth two in the *.</item>
        <item>Separate numbers by two periods for results between those numbers. Example: $50..$100.</item>
        <item>Get results from certain sites. Example: olympics site:cnn.com.</item>
        <item>Find pages that link to a certain page. Example: link:youtube.com.</item>
        <item>Find sites that are similar to a web address you already know. Example: related:time.com. </item>
        <item>Find pages that might use one of several words. Example: A Bugs Life OR Antz</item>
        <item>Get web analytics for an address. Example: info:imbd.com.</item>
        <item>See what a page looks like the last time Google visited the site. Example: cache:cnn.com</item>
    </string-array>
